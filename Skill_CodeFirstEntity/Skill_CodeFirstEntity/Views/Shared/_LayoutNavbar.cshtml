
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <style>
        ul {
            list-style-type: none;
            margin: 0;
            padding: 0;
            overflow: hidden;
            background-color: #333;
        }

        li {
            float: left;
        }

            li a {
                display: block;
                color: white;
                text-align: center;
                padding: 14px 16px;
                text-decoration: none;
            }

                li a:hover {
                    background-color: #111;
                }
    </style>
</head>
<body>

    <ul>
        <li><a href="#home">Entity Dersleri</a></li>
        <li><a href="#news">Yetenek Listesi</a></li>
        <li><a href="#contact">Yeni Yetenek</a></li>
        <li><a href="#about">Siteye Git</a></li>
    </ul>
    <div>
        @RenderBody()
    </div>
</body>
</html>



